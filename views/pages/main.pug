block room-ads
    div(class="ads")
        div(class="title")
            h1 РАЗРАБОТКА И ИЗГОТОВЛЕНИЕ ИНТЕРЬЕРНОЙ РЕКЛАМЫ
        div(id="room-one")
            div(onclick="moveTo('/navs')")
            div
